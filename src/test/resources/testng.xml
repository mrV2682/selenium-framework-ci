<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Selenium Framework Suite"
       parallel="classes"
       thread-count="2">

    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>

    <test name="DuckDuckGo Parallel Tests">
        <classes>
            <class name="tests.DuckDuckGoSearchTest"/>
            <class name="tests.DuckDuckGoTitleTest"/>
        </classes>
    </test>

</suite>
